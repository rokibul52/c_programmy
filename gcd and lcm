#include<stdio.h>
int main()
{
    int n1,n2,num1,num2,remainder,gcd,lcm;
    printf("Enter two numbers:");
      scanf("%d%d",&num1,&num2);
    n1 = num1;
      n2 = num1;
     
    while (remainder!=0)
    {
      remainder = n1%n2;
      n1 = n2;
      n2 = remainder;

    }
    gcd = n1;
    lcm = (num1*num2)/gcd;
    printf("GCD = %d\n",gcd);
    printf("LCM = %d",lcm);
    return 0;
}
