#include<stdio.h>
int main()
{
    int sum=0,numbers[]={10,20,30,40,50};
    for ( int index = 0; index <5; index++)
    {   
        printf("Enter number[%d]",index+1);
        scanf("%d",&numbers[index]);
    }
    for(int index=0; index<5; index++)
    {
        printf("%d\n",numbers[index]);
    }
    for(int index=0; index<5;index++)
    {
        sum=sum+numbers[index];
    }
    printf("the sum is= %d",sum);
    
}
