#include<stdio.h>
int main()
{
    int number[100]={4,5,6,3}, position;
    int size=4;
    printf("Enter the position of number where will be deleted:");
    scanf("%d", &position);
    for(int i=position-1; i<size; i++)
    {
        number[i]=number[i+1];
    }
    size--;
    for(int i=0; i<size; i++)
    {
        printf("%d ", number[i]);
    }

}
