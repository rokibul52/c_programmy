 #include<stdio.h>
 int main()
 {
    int n;
    printf("Enter array size:");
    scanf("%d",&n);
    int numbers[n];
    for(int index=0; index<n; index++)
    {
        printf("Enter for index %d:",index);
        scanf("%d",&numbers[index]);
    }
    for(int index=0; index<5; index++)
    {
        printf("%d\n",numbers[index]);
    }

 }
   
