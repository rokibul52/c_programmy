#include<stdio.h>
int main()
{
   int m,n;
   printf("Enter row and column number:");
   scanf("%d %d",&m,&n);
   int num[m][n];
   
   for(int i=0; i<m; i++)
   {
   for(int j=0; j<n; j++)
   { 
      printf("Enter index %d%d:",i,j);
      scanf("%d",&num[i][j]);
   }
   printf("\n");
   }
   for(int i=0; i<m; i++)
   {
   for(int j=0; j<n; j++)
   { 
      printf("%d",num[i][j]);
      
   }
   printf("\n");
   }

}
