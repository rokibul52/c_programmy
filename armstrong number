#include<stdio.h>
int main()
{
   int num, temp, remainder, sum=0;
   printf("Enter any number:");
   scanf("%d",&num);
   temp =num;
   while (temp!=0)
   {
  
   remainder= temp%10;
   sum=sum+(remainder*remainder*remainder);
   temp=temp/10;
   }
   if (num==sum)
   {
    printf("%d is a armstrong number",num);
   }
   else{
   printf("%d is not an armstrong number",num);
   }
    
}
