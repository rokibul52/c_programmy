
/*
#include<stdio.h>
#include<math.h>
int main()
{
 double x = 5.58;
 double result = round(x);//round এর কাজ হচ্ছে দশমিকের পর ৪ এর উপরে থাকলে দশমিকের বামে ১ যোগ করবে । 
 printf("round(%lf) = %lf\n",x,result);


    return 0;
}
